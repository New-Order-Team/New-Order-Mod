    <Projectile Name="Proj_Detpack">
        <Land_Model_Name>w_landmine.alo</Land_Model_Name>
        <Space_Model_Name>w_landmine.alo</Space_Model_Name>
        <Loop_Idle_Anim_00>No</Loop_Idle_Anim_00>
        <Is_Decoration>No</Is_Decoration>
        <!-- AJA - Max_Speed and Projectile_Grenade_Gravity interact together to determine -->
        <!-- the shape of the grenade's lobbing arc. A side-effect of adjusting these values -->
        <!-- is the maximum distance the grenade can travel! Be aware of that while adjusting -->
        <!-- these values! Projectile_Grenade_Gravity_Lob_Mod is just a multiplier on Projectile_Grenade_Gravity. -->
